{
    "name": "dominservice/laravel-cms",
    "type": "library",
    "description": "Laravel CMS Backend",
    "keywords": ["laravel", "cms", "manager"],
    "homepage": "https://github.com/dominservice/laravel-cms",
    "license": "MIT",
    "authors": [
        {
            "name": "dominservice",
            "email": "biuro@dso.biz.pl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-gd": "*",
        "laravel/framework": "^9|^10|^11|^12",
        "astrotomic/laravel-translatable": "^11.13",
        "kalnoy/nestedset": "^6.0"
    },
    "require-all": true,
    "autoload": {
        "psr-4": {
            "Dominservice\\LaravelCms\\": "src/"
        }
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 9.x", "Laravel 10.x", "Laravel 11.x"],
        "laravel": {
            "providers": [
                "Dominservice\\LaravelCms\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
